{
  "name": "editor-template-react-ts",
  "description": "A TypeScript project template that demonstrates React communication and uses Vite for bundling.",
  "version": "1.0.3",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/editorjs/template-react-ts.git"
  },
  "author": "Studio <support@example.com>",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/editorjs/template-react-ts/issues"
  },
  "homepage": "https://github.com/editorjs/template-react-ts#readme",
  "keywords": [
    "react",
    "vite",
    "typescript"
  ],
  "scripts": {
    "// === 🚀 開発コマンド ===": "",
    "start": "npm run dev",
    "dev": "vite --config vite/config.dev.mjs --mode development",
    "dev:full": "npm run firebase:cleanup && concurrently \"npm run firebase:start\" \"sleep 7 && npm run dev\"",
    "preview": "vite preview",
    "// === 🏗️ ビルド ===": "",
    "build": "vite build --config vite/config.prod.mjs --mode production",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "// === 🧹 コード品質 ===": "",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" && prettier --parser html --write \"src/**/*.svg\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\" && prettier --parser html --check \"src/**/*.svg\"",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check",
    "// === 🧪 テスト ===": "",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "// === 🛠️ ユーティリティ ===": "",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules",
    "reinstall": "npm run clean:all && npm install",
    "deps:update": "npx npm-check-updates -u && npm install",
    "// === 📋 情報表示 ===": "",
    "info": "echo '🚀 利用可能なコマンド:' && echo '  npm start          - 開発サーバー起動' && echo '  npm run dev:full   - Firebase + 開発サーバー同時起動' && echo '  npm run firebase:* - Firebase関連コマンド' && echo '    firebase:cleanup - ポート競合解決 + エミュレーター停止' && echo '    postgres:kill    - PostgreSQLプロセス強制終了' && echo '  npm run check      - 全体的なコード品質チェック'"
  },
  "dependencies": {
    "@lordicon/react": "^1.11.0",
    "@sukimastudy/shared": "file:../shared",
    "@tanstack-query-firebase/react": "^1.0.0",
    "@tanstack/react-query": "^5.0.0",
    "@testing-library/react": "^16.2.0",
    "@types/papaparse": "^5.3.16",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "lottie-web": "^5.13.0",
    "lucide-react": "^0.503.0",
    "papaparse": "^5.5.3",
    "qs": "^6.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.2.0",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "firebase-tools": "^14.1.0",
    "msw": "^2.7.3",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^5.3.1",
    "vitest": "^3.0.7"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
